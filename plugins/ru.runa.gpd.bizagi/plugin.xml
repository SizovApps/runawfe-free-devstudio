<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.importWizards">
      <wizard
            class="ru.runa.gpd.bizagi.wizard.ImportBpmnWizard"
            icon="icons/import.png"
            id="ru.runa.gpd.bizagi.wizard.import"
            name="%wizard.import.name">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="ru.runa.gpd.bizagi.actionSet"
            label="bizagi"
            visible="true">
         <action
               class="ru.runa.gpd.bizagi.action.ImportBpmnAction"
               icon="icons/import.png"
               id="ru.runa.gpd.bizagi.action.import"
               label="%action.import.label"
               menubarPath="file/export"
               style="push"
               toolbarPath="additions"
               tooltip="%action.import.label"/>
      </actionSet>
   </extension>

</plugin>
